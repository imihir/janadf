					},
					"AzureSqlDemoDBLS_connectionString": {
						"value": "[parameters('AzureSqlDemoDBLS_connectionString')]"
					},
					"AzureSqlsampleDBLS_connectionString": {
						"value": "[parameters('AzureSqlsampleDBLS_connectionString')]"
					},
					"SqlServerAdvLS_connectionString": {
						"value": "[parameters('SqlServerAdvLS_connectionString')]"
					},
					"SqlServerAdvLS_password": {
						"value": "[parameters('SqlServerAdvLS_password')]"
					},
					"SqlServerLS_connectionString": {
