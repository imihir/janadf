					"SqlServerLS_password": {
						"value": "[parameters('SqlServerLS_password')]"
					},
					"SqlServerAdvLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerAdvLS_properties_typeProperties_userName')]"
					},
					"SqlServerLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mihirdemoltiADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mihirdemoltiADF_ArmTemplate_0')]"
			]
		}
	]
